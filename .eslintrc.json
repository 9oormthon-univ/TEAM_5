{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint"
  ],
  "rules": {
    "import/no-named-as-default": 0,
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}
